#!/bin/bash

basename=`dirname $0`
# Forward to ${basename}/../packages/backend-core/bin/osmod.js

C=''

for i in "$@"; do
    case "$i" in
        *\'*)
            i=`printf "%s" "$i" | sed "s/'/'\"'\"'/g"`
            ;;
        *) : ;;
    esac
    C="$C '$i'"
done

bash -c "${basename}/../packages/backend-core/bin/osmod.js$C"
