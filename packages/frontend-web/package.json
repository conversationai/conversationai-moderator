{
  "name": "@conversationai/moderator-frontend-web",
  "version": "1.1.0",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "description": "OSMod project React.js frontend",
  "scripts": {
    "build": "npm run compile",
    "lint": "find src -name *.ts -o -name *.tsx | xargs ../../node_modules/.bin/tslint -c ../../tslint.json",
    "lint:fix": "find src -name *.ts -o -name *.tsx | xargs ../../node_modules/.bin/tslint -c ../../tslint.json --fix",
    "test": "npm run test:spec && npm run test:storybook",
    "test:spec": "jest src/**",
    "storybook": "start-storybook -p 9001 -c tooling/storybook -s public",
    "storybook:build": "build-storybook -c tooling/storybook -s public -o build/storybook",
    "test:storybook": "jest -c tooling/storybook/jest.config.json tooling/storybook/Storyshots.test.js",
    "compile": "npm run compile:lib && npm run compile:web",
    "compile:test": "../../node_modules/.bin/tsc --noEmit",
    "compile:lib": "rm -rf dist && ../../node_modules/.bin/tsc --sourceMap --outDir dist --declaration && npm run compile:lib:cleanup",
    "compile:lib:cleanup": "find dist -type f -exec sed -i -e 's/_1\\.default(/_1(/g' {} \\; && find dist -type f -exec sed -i -e 's/_1\\.default\\./_1\\./g' {} \\;",
    "compile:lib:watch": "../../node_modules/.bin/tsc --sourceMap --outDir dist --declaration --watch",
    "compile:web": "webpack --bail --progress --profile --colors --config tooling/webpack.config.production.js",
    "watch": "webpack-dev-server --config tooling/webpack.config.js --progress --colors --host 0.0.0.0"
  },
  "license": "Apache-2.0",
  "browserslist": [
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "jest": {
    "verbose": true,
    "preset": "ts-jest"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-react-constant-elements": "7.9.0",
    "@babel/plugin-transform-react-inline-elements": "7.9.0",
    "@babel/polyfill": "7.8.7",
    "@babel/preset-env": "7.9.5",
    "@babel/preset-react": "7.9.4",
    "@babel/register": "7.9.0",
    "@storybook/addon-actions": "^6.0.26",
    "@storybook/addon-storyshots": "^6.0.26",
    "@storybook/cli": "^6.0.26",
    "@storybook/react": "^6.0.26",
    "@types/jest": "24.0.23",
    "@types/sinon": "7.5.1",
    "babel-jest": "25.4.0",
    "babel-loader": "8.1.0",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-react-require": "3.1.3",
    "babel-plugin-require-context-hook": "1.0.0",
    "circular-dependency-plugin": "5.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "react-hot-loader": "4.12.20",
    "react-test-renderer": "^16.14.0",
    "sinon": "7.5.0",
    "source-map-loader": "0.2.4",
    "ts-jest": "^26.4.1",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "2.25.0"
  },
  "dependencies": {
    "@material-ui/core": "4.9.11",
    "@material-ui/icons": "4.9.1",
    "@types/aphrodite": "0.5.13",
    "@types/check-types": "7.3.1",
    "@types/enzyme": "3.10.3",
    "@types/jwt-decode": "2.2.1",
    "@types/keyboardjs": "2.4.1",
    "@types/prop-types": "15.7.3",
    "@types/react": "16.9.15",
    "@types/react-custom-scrollbars": "4.0.6",
    "@types/react-dom": "16.9.4",
    "@types/react-linkify": "1.0.0",
    "@types/react-redux": "7.1.3",
    "@types/react-router-dom": "5.1.3",
    "@types/react-virtualized-auto-sizer": "1.0.0",
    "@types/react-window": "1.8.2",
    "@types/redux-actions": "2.6.1",
    "aphrodite": "2.3.1",
    "axios": "0.19.0",
    "check-types": "11.0.0",
    "copy-to-clipboard": "3.2.0",
    "core-decorators": "0.20.0",
    "date-fns": "1.30.1",
    "enzyme": "3.10.0",
    "focus-trap-react": "6.0.0",
    "i": "^0.3.6",
    "immutable": "3.8.2",
    "jwt-decode": "2.2.0",
    "keyboardjs": "2.5.1",
    "normalize.css": "8.0.1",
    "npm": "^7.0.3",
    "prop-types": "15.7.2",
    "query-string": "6.9.0",
    "react": "^16.14.0",
    "react-a11y": "1.1.0",
    "react-custom-scrollbars": "4.2.1",
    "react-dom": "16.12.0",
    "react-draggable": "4.2.0",
    "react-linkify": "1.0.0-alpha",
    "react-perfect-scrollbar": "1.5.3",
