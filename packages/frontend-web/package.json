{
  "name": "@conversationai/moderator-frontend-web",
  "version": "1.1.0",
  "main": "dist-commonjs/index.js",
  "types": "./dist-commonjs/index.d.ts",
  "description": "OSMod project React.js frontend",
  "scripts": {
    "build": "npm run compile",
    "lint": "find src -name *.ts -o -name *.tsx | xargs ../../node_modules/.bin/tslint -c ../../tslint.json",
    "lint:fix": "find src -name *.ts -o -name *.tsx | xargs ../../node_modules/.bin/tslint -c ../../tslint.json --fix",
    "test": "npm run spec && npm run storybook:test",
    "spec": "jest src/**",
    "storybook": "start-storybook -p 9001 -c tooling/storybook -s public",
    "storybook:build": "build-storybook -c tooling/storybook -s public -o build/storybook",
    "storybook:test": "jest -c tooling/storybook/jest.config.json tooling/storybook/Storyshots.test.js",
    "compile": "npm run compile:lib && npm run compile:web",
    "compile:dist": "rm -rf dist && ../../node_modules/.bin/tsc --sourceMap --outDir dist --declaration --importHelpers",
    "compile:lib": "rm -rf dist-commonjs && ../../node_modules/.bin/tsc --sourceMap --outDir dist-commonjs --declaration --module commonjs && npm run compile:lib:cleanup",
    "compile:lib:cleanup": "find dist-commonjs -type f -exec sed -i -e 's/_1\\.default(/_1(/g' {} \\; && find dist-commonjs -type f -exec sed -i -e 's/_1\\.default\\./_1\\./g' {} \\;",
    "compile:lib:watch": "../../node_modules/.bin/tsc --sourceMap --outDir dist-commonjs --declaration --module commonjs --watch",
    "compile:web": "webpack --bail --progress --profile --colors --config tooling/webpack.config.production.js",
    "watch": "webpack-dev-server --config tooling/webpack.config.js --progress --colors --host 0.0.0.0"
  },
  "license": "Apache-2.0",
  "browserslist": [
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "jest": {
    "verbose": true,
    "preset": "ts-jest"
  },
  "dependencies": {
    "@types/express": "4.16.0",
    "express": "4.16.4",
    "immutable": "3.8.1",
    "typed-immutable-record": "0.0.6"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-transform-react-constant-elements": "7.5.0",
    "@babel/plugin-transform-react-inline-elements": "7.2.0",
    "@babel/polyfill": "7.4.4",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.5.5",
    "@material-ui/core": "4.3.2",
    "@material-ui/icons": "4.2.1",
    "@storybook/addon-actions": "5.1.11",
    "@storybook/addon-storyshots": "5.1.11",
    "@storybook/cli": "5.1.11",
    "@storybook/react": "5.1.11",
    "@types/aphrodite": "0.5.13",
    "@types/check-types": "7.3.1",
    "@types/enzyme": "3.10.2",
    "@types/fixed-data-table-2": "0.8.3",
    "@types/jwt-decode": "2.2.1",
    "@types/keyboardjs": "2.4.1",
