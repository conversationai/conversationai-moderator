    "@types/js-yaml": "3.12.1",
    "@types/lodash": "4.14.136",
    "@types/mysql": "2.15.5",
    "@types/sequelize": "3.4.43",
    "@types/yargs": "13.0.2",
    "express-winston": "2.5.1",
    "joi": "14.3.1",
    "js-yaml": "3.13.1",
    "lodash": "4.17.14",
    "mysql": "2.15.0",
    "sequelize": "3.30.4",
    "sequelize-cli": "2.7.0",
    "winston": "2.3.1",
    "yargs": "13.3.0"
  }
