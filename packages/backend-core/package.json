{
  "name": "@conversationai/moderator-backend-core",
  "description": "Shared Domain logic and models for OSMod",
  "version": "1.0.6",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "npm run mocha",
    "test:watch": "npm run compile:watch | npm run mocha:watch",
    "mocha": "../../node_modules/.bin/mocha 'dist/test/**/*.spec.js' --recursive --timeout 10000",
    "mocha:watch": "../../node_modules/.bin/mocha --watch 'dist/test/**/*.spec.js' --recursive --timeout 10000",
    "lint": "find src -name '*.ts' | xargs ../../node_modules/.bin/tslint -c ../../tslint.json",
    "lint:fix": "find src -name '*.ts' | xargs ../../node_modules/.bin/tslint -c ../../tslint.json --fix",
    "compile": "rm -rf dist && ../../node_modules/.bin/tsc --sourceMap --outDir dist --declaration && cp -R src/migrations dist/",
    "compile:watch": "cp -R src/migrations dist/ && ../../node_modules/.bin/tsc --sourceMap --outDir dist --declaration --watch",
    "build": "npm run compile"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": "6.11.0"
  },
  "dependencies": {
    "@conversationai/moderator-config": "1.0.3",
    "@types/bluebird": "3.5.2",
    "@types/express": "4.0.35",
    "@types/joi": "10.0.1",
    "@types/jsonwebtoken": "7.2.0",
    "@types/lodash": "4.14.104",
    "@types/mysql": "0.0.31",
    "@types/passport": "^0.4.3",
    "@types/passport-google-oauth": "1.0.33",
    "@types/passport-jwt": "2.0.19",
    "@types/redis": "0.12.36",
		"@types/request": "2.47.0",
    "@types/sequelize": "3.4.43",
    "@types/striptags": "0.0.5",
    "@types/underscore.string": "0.0.30",
    "@types/winston": "2.3.0",
    "bluebird": "3.5.0",
    "body-parser": "1.17.1",
    "canvas-prebuilt": "1.6.5-prerelease.1",
    "compression": "1.6.2",
    "cors": "2.8.1",
    "express": "4.15.2",
    "express-winston": "2.3.0",
    "helmet": "3.5.0",
    "joi": "10.3.1",
    "jsonwebtoken": "7.3.0",
    "lodash": "4.17.4",
    "moment": "2.21.0",
    "mysql": "2.13.0",
    "opentype.js": "0.6.9",
    "passport-google-oauth20": "1.0.0",
    "passport-jwt": "2.2.1",
    "redis": "2.7.1",
    "request": "2.81.0",
    "sequelize": "3.30.4",
    "sequelize-cli": "2.7.0",
    "striptags": "3.0.1",
    "underscore.string": "3.3.4",
    "winston": "2.3.1"
  }
}
