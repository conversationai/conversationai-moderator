{
  "name": "@conversationai/moderator-backend-api",
  "description": "API Endpoints for OSMod",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "version": "1.1.0",
  "scripts": {
    "test": "npm run mocha",
    "test:watch": "npm run compile && npm run compile:watch | npm run mocha:watch",
    "mocha": "WORKER_RUN_IMMEDIATELY=true ../../node_modules/.bin/mocha 'dist/test/**/*.spec.js' --timeout 200000",
    "mocha:watch": "WORKER_RUN_IMMEDIATELY=true ../../node_modules/.bin/mocha --watch 'dist/test/**/*.spec.js' --recursive --timeout 10000",
    "start": "node ./server.js",
    "lint": "find src -name '*.ts' | xargs ../../node_modules/.bin/tslint -c ../../tslint.json",
    "lint:fix": "find src -name '*.ts' | xargs ../../node_modules/.bin/tslint -c ../../tslint.json --fix",
    "compile": "rm -rf dist && ../../node_modules/.bin/tsc --sourceMap --outDir dist --declaration",
    "compile:watch": "../../node_modules/.bin/tsc --sourceMap --outDir dist --declaration --watch --preserveWatchOutput",
    "build": "npm run compile",
    "watch": "nodemon --delay 5 --inspect=5859 --watch 'dist/**/*.js' --ignore 'test' dist/server.js"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@conversationai/moderator-backend-core": "1.1.0",
    "@conversationai/moderator-backend-queue": "1.1.0",
    "@conversationai/moderator-config": "1.1.0",
    "@conversationai/moderator-frontend-web": "1.1.0",
    "@conversationai/moderator-jsonapi": "1.1.0",
    "@types/bluebird": "3.5.25",
    "@types/express": "4.16.0",
    "@types/express-ws": "3.0.0",
    "@types/joi": "10.4.0",
    "@types/lodash": "4.14.120",
    "@types/mysql": "2.15.5",
    "@types/passport": "0.4.5",
    "@types/passport-google-oauth2": "0.1.2",
    "@types/passport-jwt": "3.0.1",
    "@types/qs": "6.5.1",
    "@types/randomstring": "1.1.6",
    "@types/request": "2.47.0",
    "@types/sequelize": "3.4.43",
    "@types/underscore.string": "0.0.30",
    "bluebird": "3.5.3",
    "canvas": "2.2.0",
    "express": "4.16.4",
    "express-ws": "4.0.0",
    "googleapis": "32.0.0",
    "joi": "10.5.2",
    "lodash": "4.17.11",
    "moment": "2.21.0",
    "mysql": "2.15.0",
    "nodemon": "1.18.3",
    "passport": "0.4.0",
    "passport-google-oauth20": "1.0.0",
    "passport-jwt": "4.0.0",
    "qs": "6.6.0",
    "randomstring": "1.1.5",
    "request": "2.87.0",
    "sequelize": "3.30.4",
    "underscore.string": "3.3.4"
  }
}
